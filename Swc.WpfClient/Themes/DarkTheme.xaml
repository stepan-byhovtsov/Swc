<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Foreground" Color="#CFCFCF"></SolidColorBrush>
    
    <SolidColorBrush x:Key="SelectedForeground" Color="#FFFFFF"></SolidColorBrush>
    
    <SolidColorBrush x:Key="Background" Color="#1A1A1A"></SolidColorBrush>
    
    <SolidColorBrush x:Key="WindowTopBackground" Color="#1D1D1D"></SolidColorBrush>
    
    <SolidColorBrush x:Key="ButtonMouseOverBackground" Color="#206080"></SolidColorBrush>
    
    <SolidColorBrush x:Key="ListSelectedBackground" Color="#206080"></SolidColorBrush>
    
    <SolidColorBrush x:Key="ListMouseOverBackground" Color="#353535"></SolidColorBrush>
    
    <SolidColorBrush x:Key="ButtonPressedBackground" Color="#080D20"></SolidColorBrush>
    
    <SolidColorBrush x:Key="SlightlyAccentedBackground" Color="#252525"></SolidColorBrush> 
    
    <SolidColorBrush x:Key="AccentedBackground" Color="#292929"></SolidColorBrush> 
    
    <SolidColorBrush x:Key="Border" Color="#6F6F6F"></SolidColorBrush>
    
    <SolidColorBrush x:Key="MenuBarBackground" Color="#202020"></SolidColorBrush>
    
    <SolidColorBrush x:Key="ErrorBackground" Color="#AF4F4F"></SolidColorBrush>
</ResourceDictionary>
